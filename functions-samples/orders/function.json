{
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post",
        "get"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "orders",
      "databaseName": "store",
      "collectionName": "orders",
      "createIfNotExists": true,
      "connectionStringSetting": "OrdersDB"
    }
  ]
}
